<h1>Registered Users</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Id</th>
      <th>Username</th>
      <th>Name</th>
      <th>Registered At</th>
    </tr>
  </thead>

  <tbody>
    {{#each model as |user|}}
      <tr>
        <td>{{user.id}}</td>
        <td>{{#link-to 'user.index' user.id}}{{user.username}}{{/link-to}}</td>
        <td>{{user.name}}</td>
        <td>{{moment-format user.createdAt}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

{{page-numbers content=model}}
